import java.io.*;
import java.util.*;
public class main{
    public static void main(String[] args){
        int []arr={11,2,3,45,61,90,4,1,455,324324};
        
        int min=arr[0];
        int max=arr[0];
        
        for(int i=0;i<arr.length;i++){
            if(min>arr[i]){
                min=arr[i];
            }
            if(max<arr[i]){
                max=arr[i];
            }
        }
  System.out.println("max "+max);
  System.out.println("min "+min);
    }
    
}
